#include "LevelSetSim.h"

LevelSetSim2D::LevelSetSim2D(float width, int nx, int ny)
: width_(width), nx_(nx), ny_(ny), h_(width / (float) nx)
{
	u_.resize((nx_ + 1) * ny_);
	u_temp_.resize((nx_ + 1) * ny_);
	v_.resize((nx_ + 1) * ny_);
	v_temp_.resize((nx_ + 1) * ny_);

}

